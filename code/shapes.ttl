@prefix dash: <http://datashapes.org/dash#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#>  .

###
## TYPES
# ID
# value:string (1,1)
schema:ID 
    a sh:NodeShape, owl:Class ;
    sh:property [
        sh:path schema:value ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] ;
    sh:closed true ;
    sh:ignoredProperties (
        rdf:type
        rdfs:subClassOf
    ) ;
    .

# IDREF
schema:IDREF
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:ID ;
    .

# SId
# value:string (1,1)
schema:SId 
    a sh:NodeShape, owl:Class ;
    sh:property [
        sh:path schema:value ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] ;
    sh:closed true ;
    sh:ignoredProperties (
        rdf:type
        rdfs:subClassOf
    ) ;
    .

# SIdRef
schema:SIdRef 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SId ;
    .

# UnitSId
schema:UnitSId 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SId ;
    .

# UnitSIdRef
schema:UnitSIdRef 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:UnitSId ;
    .

# PortSId
schema:PortSId 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SId ;
    .

# PortSIdRef
schema:PortSIdRef 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:PortSId ;
    .

# LocalSId
schema:LocalSId 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SId ;
    .

# SBOTerm
# value:string (1,1)
schema:SBOTerm 
    a sh:NodeShape, owl:Class ;
    sh:property [
        sh:path schema:value ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] ;
    sh:closed true ;
    sh:ignoredProperties (
        rdf:type
        rdfs:subClassOf
    ) ;
    .

# SBase (abstract)
# id:SId (0,1) 
# name:string (0,1)  
# metaid:ID (0,n)
# sboTerm:SBOTerm (0,n)
# listOfReplacedElements:ListOfReplacedElements (0,1)  
# replacedBy:ReplacedBy (0,1)  
schema:SBase
    a sh:NodeShape, owl:Class ;
    sh:property [
        sh:path schema:id ;
        sh:maxCount 1 ;
        sh:class schema:SId ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:name ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ; 
    ] ; 
    sh:property [
        sh:path schema:metaid ;
        sh:class schema:ID ;
        sh:nodeKind sh:IRI ;
    ] ; 
    sh:property [
        sh:path schema:sboTerm ;
        sh:class schema:SBOTerm ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:listOfReplacedElements ;
        sh:maxCount 1 ;
        sh:datatype xsd:ListOfReplacedElements ; 
    ] ;
    sh:property [
        sh:path schema:replacedBy ;
        sh:maxCount 1 ;
        sh:datatype xsd:ReplacedBy ; 
    ] ;
    .
###    
## COMPONENTS
# SBaseRef 
# portRef:PortSIdRef (0,1)  
# idRef:SIdRef (0,1)
# unitRef:UnitSIdRef (0,1)
# metaIdRef:IDREF (0,1)
# sBaseRef:SBaseRef (0,1)
schema:SBaseRef
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:portRef ;
        sh:maxCount 1 ;
        sh:class schema:PortSIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:idRef ;
        sh:maxCount 1 ;
        sh:class schema:SIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:unitRef ;
        sh:maxCount 1 ;
        sh:class schema:UnitSIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:metaIdRef ;
        sh:maxCount 1 ;
        sh:class schema:IDREF ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:sBaseRef ;
        sh:maxCount 1 ;
        sh:class schema:SBaseRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    . 

# SBML
# xmlns:URI (1,n)
# level:int (1,n)
# version:int (1,n)
# listOfExternalModelDefinitions:ListOfExternalModelDefinitions (0,1)
# listOfModelDefinitions:ListOfModelDefinitions (0,1)
# model:Model (0,1)
schema:Sbml
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:xmlns ;
        sh:minCount 1 ;
        sh:datatype xsd:anyURI ;
    ] ;
    sh:property [
        sh:path schema:level ;
        sh:minCount 1 ;
        sh:datatype xsd:integer ;
    ] ;
    sh:property [
        sh:path schema:version ;
        sh:minCount 1 ;
        sh:datatype xsd:integer ;
    ] ;
    sh:property [
        sh:path schema:listOfExternalModelDefinitions ;
        sh:maxCount 1 ;
        sh:class schema:ListOfExternalModelDefinitions ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:listOfModelDefinitions ;
        sh:maxCount 1 ;
        sh:class schema:ListOfModelDefinitions ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:model ;
        sh:maxCount 1 ;
        sh:class schema:Model ;
        sh:nodeKind sh:IRI ;
    ] ;
    .

# ListOfExternalModelDefinitions
# externalModelDefinition:ExternalModelDefinition (1,n)
schema:ListOfExternalModelDefinitions
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:externalModelDefinition ;
        sh:minCount 1 ;
        sh:class schema:ExternalModelDefinition ;
        sh:nodeKind sh:IRI ;
    ] ;
    .

# ExternalModelDefinition
# id:SId (1,1) 
#Â source:URI (1,1)
# modelRef:SIdRef (0,1) 
# md5:string (0,1) 
schema:ExternalModelDefinition 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:id ;
        sh:minCount 1 ;
        sh:class schema:SId ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:source ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:anyURI ;
    ] ;
    sh:property [
        sh:path schema:modelRef ;
        sh:maxCount 1 ;
        sh:class schema:SIdRef;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:md5 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] ;
    .

# ListOfModelDefinitions
# modelDefinition:Model (1,n)
schema:listOfModelDefinitions 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:modelDefinition ;
        sh:minCount 1 ;
        sh:class schema:Model ;
        sh:nodeKind sh:IRI ;
    ] ;
    .

# Model
# substanceUnits:UnitSIdRef (0,n)
# timeUnits:UnitSIdRef (0,n)
# volumeUnits:UnitSIdRef (0,n)
# areaUnits:UnitSIdRef (0,n)
# lengthUnits:UnitSIdRef (0,n)
# extentUnits:UnitSIdRef (0,n)
# conversionFactor:SIdRef (0,n)
# listOfUnitDefinitions:ListOfUnitDefinitions (0,1)
# listOfCompartments:ListOfCompartments (0,1)
# listOfSpecies:ListOfSpecies (0,1)
# listOfParameters:ListOfParameters (0,1)
# listOfSubmodels:ListOfSubmodels (0,1)
# listOfPorts:ListOfPorts (0,1)
schema:Model 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:substanceUnits ;
        sh:class schema:UnitSIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:timeUnits ;
        sh:class schema:UnitSIdRef ;
        sh:nodeKind sh:IRI ; 
    ] ;
    sh:property [
        sh:path schema:volumeUnits ;
        sh:class schema:UnitSIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:areaUnits ;
        sh:class schema:UnitSIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:lengthUnits ;
        sh:class schema:UnitSIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:extentUnits ;
        sh:class schema:UnitSIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:conversionFactor ;
        sh:class schema:SIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:listOfUnitDefinitions ;
        sh:class schema:ListOfUnitDefinitions ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path schema:listOfCompartments ;
        sh:class schema:ListOfCompartments ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path schema:listOfSpecies ;
        sh:class schema:ListOfSpecies ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path schema:listOfParameters ;
        sh:class schema:ListOfParameters ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path schema:listOfSubmodels ;
        sh:class schema:ListOfSubmodels ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path schema:listOfPorts;
        sh:class schema:ListOfPorts ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    .

# ListOfUnitDefinitions
# unitDefinition:UnitDefinition (0,n)
schema:ListOfUnitDefinitions
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:unitDefinition ;
        sh:class schema:UnitDefinition ;
        sh:nodeKind sh:IRI ;
    ] ;
    .

# UnitDefinition
# id:UnitSId (1,1)
# listOfUnits:ListOfUnits (0,1)
schema:UnitDefinition
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:id ;
        sh:minCount 1 ;
        sh:class schema:UnitSId ;
        sh:nodeKind sh:IRI ;
    ] ; 
    sh:property [
        sh:path schema:listOfUnits ;
        sh:class schema:ListOfUnits ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    .

# ListOfUnitsShape
# unit:Unit (0,n)
schema:ListOfUnits
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:unit ;
        sh:class schema:Unit ;
        sh:nodeKind sh:IRI ;
    ] ;
    .

# Unit 
# kind:UnitSId in { ampere, avogadro, becquerel, candela, coulomb, dimensionless,
#                 farad, gram, gray, henry, hertz, item, joule, kelvin, kilogram,
#                 litre, lumen, lux, metre, mole, newton, ohm, pascal, radian, 
#                 second, siemens, sievert, steradian, tesla, volt, watt, weber } (0,n)
# multiplier:decimal (0,n)
# scale:integer (0,n)
# exponent:decimal (0,n)
schema:Unit
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:kind ;
        sh:class schema:UnitSId ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:multiplier ;
        sh:datatype xsd:decimal ;
    ] ; 
    sh:property [
        sh:path schema:scale ;
        sh:datatype xsd:integer ;
    ] ;
    sh:property [
        sh:path schema:exponent ;
        sh:datatype xsd:decimal ;
    ] ;
    .

# ListOfCompartments
# compartment:Compartment (0,n)
schema:ListOfCompartments 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:compartment ;
        sh:class schema:Compartment ;
        sh:nodeKind sh:IRI ;
    ] ;
    .

# Compartment
# id:SId (1,1) 
# spatialDimensions:decimal (0,n)
# size:decimal (0,n)
# units:UnitSIdRef (0,n)
# constant:boolean (1,1)
schema:Compartment
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:id ;
        sh:minCount 1 ;
        sh:class schema:SId ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:spatialDimensions ;
        sh:datatype xsd:decimal ;
    ] ; 
    sh:property [
        sh:path schema:size ;
        sh:datatype xsd:decimal ;
    ] ; 
    sh:property [
        sh:path schema:units ;
        sh:class schema:UnitSIdRef ;
        sh:nodeKind sh:IRI ;
    ] ; 
    sh:property [
        sh:path schema:constant ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:boolean ;
    ] ; 
    .

# ListOfSpecies
# species:Species (0,n)
schema:ListOfSpecies
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:species ;
        sh:class schema:Species ;
        sh:nodeKind sh:IRI ;
    ] ;
    .

# Species
# id:SId (1,1) 
# compartment:SIdRef (0,n)
# initialAmount:double (0,1)
# initialConcentration:double (0,1)
# substanceUnits:UnitSIdRef (0,n)
# hasOnlySubstanceUnits:boolean (0,1)
# boundaryCondition:boolean (0,1)
# constant:boolean (0,1)
# conversionFactor:SIdRef (0,n)
schema:Species 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:id ;
        sh:minCount 1 ;
        sh:class schema:SId ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:compartment ;
        sh:class schema:SIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path schema:initialAmount ;
        sh:datatype xsd:double ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path schema:initialConcentration;
        sh:datatype xsd:double ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path schema:substanceUnits ;
        sh:class schema:UnitSIdRef ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        sh:path schema:hasOnlySubstanceUnits ;
        sh:maxCount 1 ;
        sh:datatype xsd:boolean ;
    ] ;
    sh:property [
        sh:path schema:boundaryCondition ;
        sh:maxCount 1 ;
        sh:datatype xsd:boolean ;
    ] ;
    sh:property [
        sh:path schema:constant ;
        sh:maxCount 1 ;
        sh:datatype xsd:boolean ;
    ] ;
    sh:property [
        sh:path schema:conversionFactor ;
        sh:class schema:SIdRef ;
        sh:nodeKind sh:IRI ;
    ] ;
    .

# ListOfParameters
# parameter:Parameter (0,n)
schema:ListOfParameters 
    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:parameter ;
        sh:class schema:Parameter ;
        sh:nodeKind sh:IRI ;
    ] ;
    .

# Parameter
# id:SId (1,1) 
