    a sh:NodeShape, owl:Class ;
    rdfs:subClassOf schema:SBase ;
    sh:property [
        sh:path schema:id ;
        sh:minCount 1 ;
        sh:class schema:SId ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property[
        sh:path schema:modelRef ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:class schema:SIdRef ;
        sh:nodeKind sh:IRI ;
    ] ; 
    sh:property[
        sh:path schema:timeConversionFactor ;
        sh:maxCount 1 ;
        sh:class schema:SIdRef ;
        sh:nodeKind  sh:IRI ;
    ] ;
    sh:property[
        sh:path schema:extentConversionFactor ;
        sh:maxCount 1 ;
        sh:class schema:SIdRef ;
        sh:nodeKind  sh:IRI ;
    ] ;
    sh:property[
        sh:path schema:listOfDeletions ;
        sh:maxCount 1 ;
        sh:class schema:ListOfDeletions ;
        sh:nodeKind  sh:IRI ;
    ] ;
    . 

