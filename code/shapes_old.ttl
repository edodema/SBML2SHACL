# Unit 
# kind:enumerate( ampere, avogadro, becquerel, candela, coulomb, dimensionless,
#                 farad, gram, gray, henry, hertz, item, joule, kelvin, kilogram,
#                 litre, lumen, lux, metre, mole, newton, ohm, pascal, radian, 
#                 second, siemens, sievert, steradian, tesla, volt, watt, weber )
# multiplier:decimal
# scale:integer
# exponent:integer
ex:UnitShape a sh:NodeShape ;
    rdfs:subClassOf ex:SBaseShape ;
    sh:property [
        sh:path ex:id ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        sh:path ex:name ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ; 
    ] ; 
    sh:property [
        sh:path ex:metaid ;
        sh:datatype xsd:string ; 
    ] ; 
    sh:property [
        sh:path ex:sboTerm ;
        sh:datatype xsd:string ; 
    ] ;
    sh:targetClass ex:Unit ;
    sh:property [
        sh:path ex:kind ;
        sh:in (
            "ampere"
            "avogadro"
            "becquerel"
            "candela"
            "coulomb"
            "dimensionless"
            "farad"
            "gram"
            "gray"
            "henry"
            "hertz"
            "item"
            "joule"
            "kelvin"
            "kilogram"
            "litre"
            "lumen"
            "lux"
            "metre"
            "mole"
            "newton"
            "ohm"
            "pascal"
            "radian"
            "second"
            "siemens"
            "sievert"
            "steradian"
            "tesla"
            "volt"
            "watt"
            "weber"
        ) ;
    ] ;
    sh:property [
        sh:path ex:multiplier ;
        sh:datatype xsd:decimal ;
    ] ; 
    sh:property [
        sh:path ex:scale ;
        sh:datatype xsd:integer ;
    ] ;
    sh:property [
        sh:path ex:exponent ;
        sh:datatype xsd:integer ;
    ] ;
    sh:closed true ;
	sh:ignoredProperties ( rdf:type rdfs:subClassOf ) .

