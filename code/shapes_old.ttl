@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex:	<http://example.com/ns#> .
@prefix sbml: <http://www.sbml.org/sbml/level3/version2/core> .

# SBase (abstract)
# id:string (0,1) 
# name:string (0,1)  
# metaid:string (0,n)
# sboTerm:string (0,n)
ex:SBaseShape a sh:NodeShape ;
    sh:property [
        sh:path ex:id ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        sh:path ex:name ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ; 
    ] ; 
    sh:property [
        sh:path ex:metaid ;
        sh:datatype xsd:string ; 
    ] ; 
    sh:property [
        sh:path ex:sboTerm ;
        sh:datatype xsd:string ; 
    ] ;
    sh:closed true ;
	sh:ignoredProperties ( rdf:type rdfs:subClassOf ) .

# SBML
# xmlns:IRI (1,n)
# level=3:int (1,n)
# version=2:int (1,n)
# model:Model (0,1)
ex:SBMLShape a sh:NodeShape ;
    rdfs:subClassOf ex:SBaseShape ;
    sh:property [
        sh:path ex:id ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        sh:path ex:name ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ; 
    ] ; 
    sh:property [
        sh:path ex:metaid ;
        sh:datatype xsd:string ; 
    ] ; 
    sh:property [
        sh:path ex:sboTerm ;
        sh:datatype xsd:string ; 
    ] ;
    sh:targetClass ex:SBML ;
    sh:property [
        sh:path ex:xmlns ;
        sh:minCount 1 ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path ex:level ;
        sh:minCount 1 ;
        sh:hasValue 3 ;
        sh:datatype xsd:integer ;
    ] ;
    sh:property [
        sh:path ex:version ;
        sh:minCount 1 ;
        sh:hasValue 2 ;
        sh:datatype xsd:integer ;
    ] ;
    sh:property [
        sh:path ex:model ;
        sh:maxCount 1 ;
        sh:class ex:Model ;
        sh:nodeKind sh:IRI ;
    ] .

# Model
# substanceUnits:string (0,n)
# timeUnits:string (0,n)
# volumeUnits:string (0,n)
# areaUnits:string (0,n)
# lengthUnits:string (0,n)
# extentUnits:string (0,n)
# conversionFactor:Parameter (0,n)
# listOfUnitDefinitions:ListOfUnitDefinitions (0,1)
# listOfCompartments:ListOfCompartments (0,1)
# listOfSpecies:ListOfSpecies (0,1)
# listOfParameters:ListOfParameters (0,1)
ex:ModelShape a sh:NodeShape ;
sh:property [
        sh:path ex:id ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] ;
    sh:property [
        sh:path ex:name ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ; 
    ] ; 
    sh:property [
        sh:path ex:metaid ;
        sh:datatype xsd:string ; 
    ] ; 
    sh:property [
        sh:path ex:sboTerm ;
        sh:datatype xsd:string ; 
    ] ;
    rdfs:subClassOf ex:SBaseShape ;
    sh:targetClass ex:Model ;
    sh:property [
        sh:path ex:substanceUnits ;
        sh:datatype xsd:string ; 
    ] ;
    sh:property [
        sh:path ex:timeUnits ;
        sh:datatype xsd:string ; 
    ] ;
    sh:property [
        sh:path ex:volumeUnits ;
        sh:datatype xsd:string ; 
    ] ;
    sh:property [
        sh:path ex:areaUnits ;
        sh:datatype xsd:string ; 
    ] ;
    sh:property [
        sh:path ex:lengthUnits ;
        sh:datatype xsd:string ; 
    ] ;
    sh:property [
        sh:path ex:extentUnits ;
        sh:datatype xsd:string ; 
    ] ;
    sh:property [
        sh:path ex:conversionFactor ;
        sh:class ex:Parameter ;
        sh:nodeKind sh:IRI ;
    ] ;
    sh:property [
        sh:path ex:listOfUnitDefinitions ;
        sh:class ex:ListOfUnitDefinitions ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path ex:listOfCompartments ;
        sh:class ex:ListOfCompartments ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path ex:listOfSpecies ;
        sh:class ex:ListOfSpecies ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    sh:property [
        sh:path ex:listOfParameters ;
        sh:class ex:ListOfParameters ;
        sh:nodeKind sh:IRI ;
        sh:maxCount 1 ;
    ] ;
    sh:closed true ;
	sh:ignoredProperties ( rdf:type rdfs:subClassOf ) .

